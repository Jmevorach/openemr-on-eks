extends: relaxed

rules:
  line-length: disable
  indentation: disable
  commas: disable
  comments: disable
  document-start: disable
  empty-lines: disable
  empty-values: disable
  hyphens: disable
  key-duplicates: disable
  key-ordering: disable
  new-line-at-end-of-file: disable
  octal-values: disable
  quoted-strings: disable
  truthy: disable
